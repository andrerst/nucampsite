{
  "name": "nucampsite",
  "version": "1.0.0",
  "description": "This is a website for fictional campsites review website, called Nucamp",
  "main": "index.html",
  "scripts": {
    "lite": "lite-server",
    "scss": "sass css:css",
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'"
  },
  "author": "Andre Stefanini",
  "license": "ISC",
  "devDependencies": {
    "lite-server": "^2.6.1",
    "onchange": "^7.0.0",
    "parallelshell": "3.0.1"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "sass": "^1.63.6"
  }
}
